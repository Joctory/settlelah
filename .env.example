# SettleLah Environment Variables
# Copy this file to .env and fill in your actual values

# Authentication
APP_PASSCODE=123456
SETTLELAH_AUTH_TOKEN=your_random_secure_token_here

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here

# Mindee OCR Service (for receipt scanning)
MINDEE_API_KEY=your_mindee_api_key_here

# Development Mode (set to true for development)
SETTLELAH_DEV_MODE=false

# Node Environment
NODE_ENV=development