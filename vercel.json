{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/calculate", "dest": "index.js" },
    { "src": "/result/(.*)", "dest": "index.js" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
